[url "https://github.com/"]
  insteadOf = gh:

[color]
    ui = true

[color "branch"]
  current = yellow reverse
  local = yello
  remote = green

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold

[color "status"]
  added = yellow
  changed = green
  untracked = red

