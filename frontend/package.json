{
  "name": "frontend",
  "version": "0.0.0",
  "license": "GNU AGPLv3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "start:pwa": "bash ./tools/run-pwa.sh",
    "build": "ng build",
    "pretest": "npm run lint",
    "test": "ng test --watch=false --code-coverage",
    "lint": "ng lint --type-check && npm run stylelint",
    "pree2e": "webdriver-manager update --gecko=false",
    "e2e": "ng e2e --proxy-config proxy.conf.json",
    "stylelint": "stylelint --config ./.stylelintrc.json --syntax scss --color ./src/**/*.scss",
    "clean": "rm -rf ./dist ./coverage",
    "dist": "ng build --prod --build-optimizer --aot --no-sourcemap --extract-licenses=false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.5",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/flex-layout": "5.0.0-beta.14",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/material": "^5.2.5",
    "@angular/material-moment-adapter": "^5.2.5",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@angular/service-worker": "^5.2.9",
    "chart.js": "^2.7.2",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "moment": "^2.20.1",
    "rxjs": "^5.5.2",
    "typescript": "2.6.x",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^5.2.1",
    "@angular/language-service": "^5.2.1",
    "@types/jasmine": "^2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.1.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "stylelint": "^8.2.0",
    "stylelint-config-sass-guidelines": "^3.1.0",
    "stylelint-scss": "^2.1.0",
    "ts-node": "~3.2.0",
    "tslint": "^5.9.1"
  }
